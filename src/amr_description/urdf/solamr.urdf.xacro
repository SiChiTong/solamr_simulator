<!-- #####   BEGIN  ##### -->
<!-- #####  A2 PART ##### -->
<!-- #####   BEGIN  ##### -->

  <joint name="A2_top_joint" type="fixed">
    <parent link="chassis"/>
    <child link="A2_top" />
    <origin xyz="0 0 0.18" rpy="0 0 0"/>
  </joint>

  <link name="A2_top">
    <visual>
      <origin xyz="0.0 0 0.0" rpy="0 0 -${PI/2}"/>
      <geometry>
        <mesh filename="package://amr_description/meshes/solamr/A2_top_H.stl" scale="0.01 0.01 0.01"/>
        <!--
        <mesh filename="package://amr_description/meshes/solamr/A2_top_extr10mm_H.stl" scale="0.01 0.01 0.01"/>
-->
      </geometry>
      <material name="white"/>
    </visual>

    <collision>
      <max_contacts>1</max_contacts>
      <origin xyz="0.0 0 0.0" rpy="0 0 -${PI/2}"/>
      <geometry>
        <mesh filename="package://amr_description/meshes/solamr/A2_top_H.stl" scale="0.01 0.01 0.01"/>
        <!--
        <mesh filename="package://amr_description/meshes/solamr/A2_top_extr10mm_H.stl" scale="0.01 0.01 0.01"/>
-->
      </geometry>
    </collision>
    
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1.5"/>
      <inertia ixx="2.15625e-02" ixy="0" ixz="0"
               iyy="2.15625e-02" iyz="0"
               izz="2.25e-02" />
    </inertial>
  </link>

  <transmission name="blocker_tran" type="SimpleTransmission">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="blocker_joint">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="blocker_motor">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
      <mechanicalReduction>500</mechanicalReduction>
    </actuator>
  </transmission>

  <joint name="blocker_joint" type="continuous">
    <parent link="chassis"/>
    <child link="blocker"/>
    <origin xyz="0.077 0 .2" rpy="0 1.57 0"/>
    <axis xyz="0 0 1"/>
  </joint>

  <link name="blocker">
    <visual>
      <!--
      <origin xyz=".02 0 -0.0095" rpy="0 0 0"/>
-->
      <origin xyz=".015 0 -0.002" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://amr_description/meshes/solamr/blocker.stl" scale="0.009 0.009 0.009"/>
      </geometry>
      <material name="orange"/>
    </visual>

    <collision>
      <max_contacts>1</max_contacts>
      <origin xyz=".02 0 -0.00" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://amr_description/meshes/solamr/blocker.stl" scale="0.009 0.009 0.009"/>
      </geometry>
    </collision>

    <gazebo reference ="blocker">
      <material>Gazebo/Orange</material>
      <turnGravityOff>false</turnGravityOff>
    </gazebo> 

    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.5"/>
      <inertia ixx="6.721e-04" ixy="0" ixz="0"
               iyy="6.721e-04" iyz="0"
               izz="1.3141e-03" />
    </inertial>
  </link>




<!-- #####    END   ##### -->
<!-- #####  A2 PART ##### -->
<!-- #####    END   ##### -->


<!-- Begin of camera -->

  <joint name="camera_joint" type="fixed">
    <origin xyz="0.33 0 0.161" rpy="0 -0.35 0"/>
    <parent link="chassis"/>
    <child link="camera_link"/>
  </joint>

  <link name="camera_link">
    <visual>
     <origin xyz="0 0 0" rpy="1.57 0 1.57"/>
      <geometry>
          <mesh filename="package://amr_description/meshes/sensors/r200.dae" />
      </geometry>
    </visual>
    <!--
    <collision>
      <origin xyz="0.003 0.065 0.007" rpy="0 0 0"/>
      <geometry>
        <box size="0.012 0.132 0.020"/>
      </geometry>
    </collision>
-->
  </link>

  <joint name="camera_rgb_joint" type="fixed">
    <origin xyz="${r200_cam_rgb_px} ${r200_cam_rgb_py} ${r200_cam_rgb_pz}" rpy="0 0 0"/>
    <parent link="camera_link"/>
    <child link="camera_rgb_frame"/>
  </joint>
  <link name="camera_rgb_frame"/>

  <joint name="camera_rgb_optical_joint" type="fixed">
    <origin xyz="0 0 0" rpy="-1.57 0 -1.57"/>
    <parent link="camera_rgb_frame"/>
    <child link="camera_rgb_optical_frame"/>
  </joint>
  <link name="camera_rgb_optical_frame"/>

  <joint name="camera_depth_joint" type="fixed">
    <origin xyz="${r200_cam_rgb_px} ${r200_cam_rgb_py + r200_cam_depth_offset} ${r200_cam_rgb_pz}" rpy="0 0 0"/>
    <parent link="camera_link"/>
    <child link="camera_depth_frame"/>
  </joint>
  <link name="camera_depth_frame"/>

  <joint name="camera_depth_optical_joint" type="fixed">
    <origin xyz="0 0 0" rpy="-1.57 0 -1.57"/>
    <parent link="camera_depth_frame"/>
    <child link="camera_depth_optical_frame"/>
  </joint>
  <link name="camera_depth_optical_frame"/>

<!-- End of camera -->

</robot>
